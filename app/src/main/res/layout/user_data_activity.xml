<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include
            android:id="@+id/my_toolbar"
            layout="@layout/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
        </include>

        <ImageView
            android:id="@+id/imageVieww13"
            android:layout_width="457dp"
            android:layout_height="185dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/my_toolbar"
            app:srcCompat="@drawable/untitled" />

        <ImageView
            android:id="@+id/imageVieww14"
            android:layout_width="368dp"
            android:layout_height="154dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageVieww13"
            app:srcCompat="@drawable/kategorija" />

        <ImageView
            android:id="@+id/imageVieww15"
            android:layout_width="368dp"
            android:layout_height="154dp"
            android:layout_marginTop="119dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageVieww14"
            app:srcCompat="@drawable/kategorija" />

        <TextView
            android:id="@+id/textVieww12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="46dp"
            android:text="Moji podaci"
            android:textColor="@color/black"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/imageVieww15"
            app:layout_constraintEnd_toEndOf="@+id/imageVieww14"
            app:layout_constraintHorizontal_bias="0.332"
            app:layout_constraintStart_toStartOf="@+id/imageVieww14"
            app:layout_constraintTop_toTopOf="@+id/imageVieww14"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textVieww14"
            android:layout_width="209dp"
            android:layout_height="40dp"
            android:text="Pregledaj i izmjeni svoje osobne podatke"
            app:layout_constraintBottom_toTopOf="@+id/imageVieww15"
            app:layout_constraintEnd_toEndOf="@+id/imageVieww14"
            app:layout_constraintHorizontal_bias="0.547"
            app:layout_constraintStart_toStartOf="@+id/imageVieww14"
            app:layout_constraintTop_toBottomOf="@+id/textVieww12"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textVieww15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="46dp"
            android:text="Statistike"
            android:textColor="@color/black"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imageVieww15"
            app:layout_constraintHorizontal_bias="0.31"
            app:layout_constraintStart_toStartOf="@+id/imageVieww15"
            app:layout_constraintTop_toTopOf="@+id/imageVieww15" />

        <TextView
            android:id="@+id/textVieww16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Zanimljive statistike o meni"
            app:layout_constraintEnd_toEndOf="@+id/imageVieww15"
            app:layout_constraintHorizontal_bias="0.448"
            app:layout_constraintStart_toStartOf="@+id/imageVieww15"
            app:layout_constraintTop_toBottomOf="@+id/textVieww15" />

        <ImageView
            android:id="@+id/imageVieww19"
            android:layout_width="71dp"
            android:layout_height="64dp"
            app:layout_constraintBottom_toTopOf="@+id/imageVieww15"
            app:layout_constraintEnd_toStartOf="@+id/textVieww12"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageVieww14"
            app:layout_constraintTop_toBottomOf="@+id/imageVieww13"
            app:layout_constraintVertical_bias="0.836"
            app:srcCompat="@drawable/kapano" />

        <ImageView
            android:id="@+id/imageVieww20"
            android:layout_width="71dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toStartOf="@+id/textVieww15"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageVieww15"
            app:layout_constraintTop_toBottomOf="@+id/imageVieww14"
            app:srcCompat="@drawable/pivkano" />

        <TextView
            android:id="@+id/textVieww17"
            android:layout_width="366dp"
            android:layout_height="62dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            android:textAllCaps="false"
            android:textColor="@color/black"
            android:textSize="40dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageVieww13" />


        <ImageView
            android:id="@+id/imageVieww26"
            android:layout_width="24dp"
            android:layout_height="22dp"
            android:layout_marginTop="64dp"
            app:layout_constraintEnd_toEndOf="@+id/imageVieww14"
            app:layout_constraintHorizontal_bias="0.666"
            app:layout_constraintStart_toEndOf="@+id/textVieww14"
            app:layout_constraintTop_toBottomOf="@+id/imageVieww13"
            app:srcCompat="@drawable/_0758" />

        <ImageView
            android:id="@+id/imageVieww27"
            android:layout_width="24dp"
            android:layout_height="22dp"
            android:layout_marginTop="32dp"
            app:layout_constraintEnd_toEndOf="@+id/imageVieww15"
            app:layout_constraintHorizontal_bias="0.807"
            app:layout_constraintStart_toEndOf="@+id/textVieww16"
            app:layout_constraintTop_toBottomOf="@+id/imageVieww14"
            app:srcCompat="@drawable/_0758" />

        <ImageView
            android:id="@+id/removeFromFavoritesS"
            android:layout_width="43dp"
            android:layout_height="42dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textVieww17"
            app:srcCompat="@drawable/removefromfavorites" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>